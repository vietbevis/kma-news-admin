    "title": {
      "type": "string"
    },
    "summary": {
      "type": "text"
    },
    "link": {
      "type": "component",
      "component": "elements.link",
      "repeatable": false
    },
    "variants": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "news",
        "events",
        "trainning",
        "cooperation",
        "alumni",
        "slider"
      ]
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "backgroundImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "isBackgroundHighlight": {
      "type": "boolean",
      "default": false
    },
    "isLinkFullWidth": {
      "type": "boolean",
      "default": false
    }
  },
  "config": {}
}
