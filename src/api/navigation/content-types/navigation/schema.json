{
  "kind": "collectionType",
  "collectionName": "navigations",
  "info": {
    "singularName": "navigation",
    "pluralName": "navigations",
    "displayName": "Navigation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "text": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "text",
      "required": true
    },
    "navigations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::navigation.navigation",
      "mappedBy": "navigation"
    },
    "navigation": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::navigation.navigation",
      "inversedBy": "navigations"
    },
    "singlePageContent": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "pageType": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "default": "news",
      "enum": [
        "news",
        "single"
      ]
    }
  }
}
